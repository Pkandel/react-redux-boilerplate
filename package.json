{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "This is a boilerplate for react-redux application.",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel run:server watch:css lint:watch",
    "lint": "node_modules/.bin/esw webpack.config.babel.js src",
    "fix": "eslint --fix src/.; exit 0;",
    "run:server": "webpack-dev-server --env.NODE_ENV=development",
    "lint:watch": "npm run lint --watch",
    "watch:css": "node-sass src/ -o src/ --watch --recursive",
    "build": "webpack --env.NODE_ENV=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkandel/react-redux-boilerplate"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel"
  ],
  "author": "Prakash Kandel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pkandel/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/pkandel/react-redux-boilerplate#readme",
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.4",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-watch": "^5.1.2",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.8.4",
    "redux-immutable-state-invariant": "^2.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
