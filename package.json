{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "This is a boilerplate for react-redux application.",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development npm-run-all --parallel run:server watch:css lint:watch",
    "lint": "node_modules/.bin/esw webpack.config.babel.js src",
    "fix": "eslint --fix src/.; exit 0;",
    "run:server": "babel-node tools/devServer.js",
    "lint:watch": "npm run lint --watch",
    "watch:css": "node-sass src/ -o src/ --watch --recursive",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf dist",
    "build:html": "babel-node tools/buildHtml.js",
    "preprod": "npm-run-all clean-dist build:html",
    "prod": " babel-node tools/build.js",
    "postprod": "NODE_ENV=production babel-node tools/prodServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkandel/strategic-frontend"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel"
  ],
  "author": "Prakash Kandel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pkandel/strategic-frontend/issues"
  },
  "homepage": "https://github.com/pkandel/strategic-frontend#readme",
  "dependencies": {
    "compression": "^1.7.0",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili": "^0.1.4",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "dotenv-webpack": "^1.5.4",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.2",
    "expect": "^1.20.2",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.29.0",
    "jsdom": "^8.5.0",
    "lodash-webpack-plugin": "^0.11.4",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "portfinder": "^1.0.13",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "redux-immutable-state-invariant": "^2.0.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "standard": "^10.0.3",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.0"
  }
}
