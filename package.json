{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "This is a quick start kit for react and redux application using react, react-router, redux, webpack, babel and more..",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development npm-run-all --parallel run:server watch:css lint:watch test:watch",
    "lint": "node_modules/.bin/esw webpack.config.* src",
    "run:server": "babel-node tools/devServer.js",
    "lint:watch": "npm run lint -- --watch",
    "build:css": "node-sass src/ -o src/",
    "watch:css": "npm run build:css && node-sass src/ -o src/ --watch --recursive",
    "test": "mocha --reporter progress src/utils/testSetup.js \"src/tests/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf dist",
    "build:html": "babel-node tools/buildHtml.js",
    "preprod": "NODE_ENV=production npm-run-all clean-dist test lint build:html",
    "prod": " babel-node tools/build.js",
    "postpord": "babel-node tools/prodServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkandel/react-redux.git"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate",
    "webpack",
    "babel"
  ],
  "author": "Prakash Kandel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pkandel/react-redux/issues"
  },
  "homepage": "https://github.com/pkandel/react-redux#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "compression": "^1.7.0",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "dotenv-webpack": "^1.5.4",
    "eslint": "^4.1.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.2",
    "expect": "^1.20.2",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.29.0",
    "jsdom": "^8.5.0",
    "lodash-webpack-plugin": "^0.11.4",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "portfinder": "^1.0.13",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^1.3.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.0"
  }
}
