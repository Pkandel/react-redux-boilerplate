{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "This is a boilerplate for react-redux application.",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel run:server watch:css lint:watch",
    "lint": "node_modules/.bin/esw webpack.config.babel.js src",
    "fix": "eslint --fix src/.; exit 0;",
    "run:server": "webpack-dev-server --env.NODE_ENV=development",
    "lint:watch": "npm run lint --watch",
    "watch:css": "node-sass src/ -o src/ --watch --recursive",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf dist",
    "preprod": "npm-run-all clean-dist",
    "prod": "webpack --env.NODE_ENV=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkandel/react-redux-boilerplate"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel"
  ],
  "author": "Prakash Kandel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pkandel/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/pkandel/react-redux-boilerplate#readme",
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "compression": "^1.7.4",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.3",
    "colors": "^1.3.3",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-watch": "^5.1.2",
    "expect": "^24.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.0.0",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "open": "6.2.0",
    "portfinder": "^1.0.20",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.8.4",
    "redux-immutable-state-invariant": "^2.1.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
